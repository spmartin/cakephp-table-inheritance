{
    "name": "spmartin/cakephp-table-inheritance",
    "description": "CakePHP STI & CTI pattern plugin",
    "homepage": "https://github.com/spmartin/cakephp-table-inheritance",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "cakephp/orm": "^5.0"
    },
    "require-dev": {
        "php": ">=8.0",
        "phpunit/phpunit": "^11.0.7",
        "cakephp/cakephp": "^5.0",
        "cakephp/cakephp-codesniffer": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Spmartin\\TableInheritance\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests",
            "Spmartin\\TableInheritance\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
